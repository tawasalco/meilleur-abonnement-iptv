{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@utils/*": ["./src/utils/*"],
      "@content/*": ["./src/content/*"],
      "@assets/*": ["./src/assets/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
